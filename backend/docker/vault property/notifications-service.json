{
  "server.port": "8084",

  "spring.cloud.loadbalancer.ribbon.enabled": "false",

  "eureka.instance.preferIpAddress": "true",
  "eureka.instance.instance-id": "${spring.application.name}:${spring.application.instance_id:${random.value}}",
  "eureka.client.registerWithEureka": "true",
  "eureka.client.fetchRegistry": "true",
  "eureka.client.serviceUrl.defaultZone": "http://eureka-server:8761/eureka/",

  "management.endpoints.web.exposure.include": "*",
  "management.endpoints.enabled-by-default": "true",

  "spring.cloud.stream.bindings.entryChangeHandler-in-0.destination": "entry-events",
  "spring.cloud.stream.bindings.entryChangeHandler-in-0.content-type": "application/json",
  "spring.cloud.stream.bindings.entryChangeHandler-in-0.group": "notification-service",

  "spring.cloud.stream.kafka.binder.brokers": "kafka:9092",
  "spring.cloud.stream.kafka.binder.auto-create-topics": "true",
  "spring.cloud.stream.kafka.binder.consumer-properties.auto.offset.reset": "earliest",
  "spring.cloud.stream.kafka.binder.consumer-properties.enable.auto.commit": "true",
  "spring.cloud.stream.kafka.bindings.entryChangeHandler-in-0.consumer.auto-commit-offset": "true",
  "spring.cloud.stream.kafka.bindings.entryChangeHandler-in-0.consumer.start-offset": "earliest",

  "spring.data.redis.host": "redis",
  "spring.data.redis.port": "6379",

  "max.bot-token": "",

  "logging.level.org.springframework.cloud.stream": "INFO",
  "logging.level.org.apache.kafka": "WARN",
  "logging.level.org.springframework.kafka": "INFO",
  "logging.level.org.springframework.data.redis": "WARN",
  "logging.level.io.lettuce.core": "WARN",
  "logging.level.org.hackaton.max2025.notification_ms": "DEBUG",
  "logging.level.org.hackaton.max2025.notification_ms.service.NotificationSchedulerService": "INFO",
  "logging.level.org.hackaton.max2025.notification_ms.service.NotificationSenderService": "DEBUG",
  "logging.level.org.hackaton.max2025.notification_ms.service.client.EntryCacheService": "DEBUG"
}